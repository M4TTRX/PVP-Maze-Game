#include "MazeSquare.h"
MazeSquare::MazeSquare()
{

}

MazeSquare::MazeSquare(int row, int column) {	//each square starts with 4 walls and no neighbours, which changes later in construction
	leftWall = true;
	rightWall = true;
	upWall = true;
	downWall = true;
	x = row;
	y = column;
	x = column;
	y = row;
	visited = false;
	item = NULL;
}

